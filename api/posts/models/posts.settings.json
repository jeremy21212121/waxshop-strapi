{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "posts"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "body": {
      "type": "richtext",
      "required": false
    },
    "title": {
      "type": "string",
      "required": true
    },
    "published": {
      "type": "boolean",
      "required": true
    },
    "date": {
      "type": "date"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    }
  }
}
